<h1>編集画面</h1>
<div class="row">
  <div class="col-6">
    <%= render 'shared/error_messages', model: @title%>

      <%= form_with(model: @title, local: true) do |f|%> 
        <%= render 'form', f: f %>
          <%= f.submit "編集", id: 'submit', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>

