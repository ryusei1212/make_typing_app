<!DOCTYPE html>
<html>
  <head>
    <title>TypedApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body>
    <% flash.each do|message_type, message| %>
      <% message_type %><%= message %>
    <% end %>
      <%= render 'layouts/header' %>
    <%= yield %>
  </body>
</html>
